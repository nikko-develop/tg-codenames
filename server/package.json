{
  "name": "tg-codenames",
  "version": "0.0.0",
  "description": "Nikko's Codenames",
  "type": "commonjs",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "build": "nest build",
    "lint": "pnpm exec eslint",
    "migrate": "migrate-mongo",
    "migrations:up": "npm run build && npm run migrate up",
    "migrations:down": "npm run build npm run migrate down",
    "migrations:status": "npm run build npm run migrate status",
    "prebuild": "rimraf dist",
    "prepare": "husky",
    "start": "nest start",
    "start:debug": "nest start --watch --debug",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "pnpm exec jest --passWithNoTests",
    "test:cov": "pnpm exec jest --coverage=true",
    "test:watch": "pnpm exec jest --watch"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier -w"
    ]
  },
  "author": "Nikko Dev",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.9",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/platform-fastify": "^10.4.1",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "eventemitter2": "^6.4.9",
    "fastify": "^4.28.1",
    "lodash": "^4.17.21",
    "migrate-mongo": "^11.0.0",
    "mongoose": "^8.2.2",
    "nest-typed-config": "^2.9.3",
    "nestjs-request-context": "^3.0.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "ulid": "^2.3.0"
  }
}
